{
    "cuda": true,
    "database": {
        "name": "Remote",
        "base_path": "F:/Remote Sensing/Data",
        "image_bands": [0, 1, 2]
    },
    "output": {
        "results_path": "F:/Remote Sensing/Data/output",
        "checkpoints": {
            "normal": "F:/Remote Sensing/Data/output/checkpoints/normal",
            "noise": "F:/Remote Sensing/Data/output/checkpoints/noise"
        }
    },
    "training": {
        "batch_size": 64,
        "epochs": 1,
        "learning_rate": 1e-4,
        "seed": 1,
        "train_data": {
            "normal": "F:/Remote Sensing/Data/Train/Normal/",
            "noise": "F:/Remote Sensing/Data/Train/Noise/"
        },
        "val_data": {
            "normal": "F:/Remote Sensing/Data/Validation/Normal/",
            "noise": "F:/Remote Sensing/Data/Validation/Noise/"
        }
    },
    "gpu": {
        "use_multi_gpu": false,
        "gpu_ids": [0],
        "num_threads": 12
    },
    "models": {
        "SFCSR": {
            "upscale_factor": 4,
            "n_feats": 32,
            "n_module": 3
        },
        "MCNet": {
            "upscale_factor": 4,
            "n_feats": 32,
            "n_conv": 1,
            "n_colors": 3
        },
        "Propuesto": {
            "upscale_factor": 4,
            "n_feats": 16,
            "n_module": 4,
            "n_conv": 1,
            "cross_attention": {
                "query_key_reduction": 16
            },
            "denoising_block": {
                "kernel_size": 2,
                "activation": "ReLU"
            },
            "attention_block": {
                "kernel_size": 5,
                "reduction": 8
            },
            "n_colors": 3
        },
        "Propuesto2": {
            "upscale_factor": 4,
            "n_feats": 16,
            "n_module": 4,
            "n_conv": 1,
            "denoising_block": {
                "kernel_size": 3,
                "activation": "ReLU"
            },
            "attention_block": {
                "kernel_size": 5,
                "reduction": 8
            },
            "n_colors": 3
        },
        "Modificacion1": {
            "upscale_factor": 4,
            "n_feats": 16,
            "n_colors": 3,
            "n_conv": 1,
            "denoising_block": {
                "kernel_size": 3,
                "activation": "ReLU"
            },
            "attention_block": {
                "kernel_size": 5,
                "reduction": 8
            }
        },
        "Modificacion2": {
            "upscale_factor": 4,
            "n_feats": 16,
            "n_colors": 3,
            "n_conv": 1,
            "cross_attention": {
                "query_key_reduction": 8
            }
        }
    },
    "model_list": ["Propuesto2","Modificacion2","Modificacion1","Propuesto","MCNet","SFCSR"],
    "test": {
        "test_data": {
            "normal": "F:/Remote Sensing/Data/Test/Normal",
            "noise": "F:/Remote Sensing/Data/Test/Noise"
        }
    }
}
